{{#each partials.cardContext}}
    <div class ="card">
        <a href="https://github.com/Quaz711">
            <img src="{{card.pic}}" alt="">
            <h2 class="small-txt">{{card.name}}</h2>
            <p class="small-txt">{{card.sport}}</p>
            <p class="small-txt">{{card.city}}, {{card.state}}</p>
        </a>
    </div>
{{/each}}